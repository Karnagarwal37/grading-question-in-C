#include<stdio.h>
int main(){
    int marks;
    printf("enter the marks:");
    scanf("%d",&marks);
    if(marks<30 && marks>=0){
        printf("your grade is C\n");
    }
    else if(marks >=70 && marks<90){
        printf("your grade is A\n");
    }
    else if(marks >=30 && marks<70){
        printf("your grade is B\n");
    }
    else if(marks >=90 && marks<=100){
        printf("your grade is A+\n");
    }
    else {
        printf("wrong number");
}
}
